<!DOCTYPE html>
<html>
  <head>
    <title><%= titre %></title>
    <%= csrf_meta_tags %>
    <%= render 'layouts/stylesheets'%>
    <%= javascript_include_tag "jquery-1.9.1.min" %>
  </head>

  <body>
   <div class="container">
   <%= render 'layouts/header' %>
      <section class="round">
        <%= yield %>
      </section>
        <%= render 'layouts/footer' %>
        <%= debug(params) if Rails.env.development? %>
        <%= javascript_include_tag "application" %>
    </div>
  </body>
</html>
